{
    "name": "<%= appName %>",
    "version": "<%= appVersion %>",
    "description": "<%= appDescription %>",
    "scripts": {
        "develop": "webpack-dev-server --content-base build/ --hot --inline --progress --colors --port 8000",
        "distribute": "webpack --progress --colors --config webpack.config.production.js",
        "test": "karma start --single-run",
        "test-coverage": "karma start --single-run && open './coverage/PhantomJS 2.1.1 (Mac OS X 0.0.0)/index.html'",
        "test-tdd": "karma start --auto-watch",
        "test-browser": "karma start --single-run --browsers 'PhantomJS,Chrome,Firefox'",
        "node-purge": "rm -rf node_modules",
        "node-reinstall": "npm run node-purge && npm install",
        "node-packages": "npm list --depth=0",
        "node-updates": "./node_modules/.bin/ncu '/^(?!npm-shrinkwrap|jsdom).*$/'",
        "node-upgrade": "npm run node-updates -- -u",
        "node-killall": "killall node"
    },
    "author": "YOUR TEAM",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "~6.11.4",
        "babel-eslint": "~6.1.0",
        "babel-loader": "~6.2.4",
        "babel-polyfill": "~6.9.1",
        "babel-preset-es2015": "~6.9.0",
        "clean-webpack-plugin": "~0.1.8",
        "css-loader": "~0.23.1",
        "dotenv": "~2.0.0",
        "extract-text-webpack-plugin": "~1.0.1",
        "file-loader": "~0.9.0",
        "glob": "~7.0.3",
        "html-loader": "~0.4.3",
        "html-webpack-plugin": "~2.22.0",
        "inline-source-map": "~0.6.1",
        "isparta": "~4.0.0",
        "isparta-loader": "~2.0.0",
        "jasmine": "~2.4.1",
        "jasmine-core": "~2.4.1",
        "jasmine-expect": "~2.0.2",
        "jasmine-sinon": "~0.4.0",
        "jshint-stylish": "~2.2.0",
        "karma": "~0.13.15",
        "karma-babel-preprocessor": "~6.0.1",
        "karma-chrome-launcher": "~1.0.1",
        "karma-coverage": "~1.0.0",
        "karma-firefox-launcher": "~1.0.0",
        "karma-html2js-preprocessor": "~1.0.0",
        "karma-jasmine": "~1.0.2",
        "karma-junit-reporter": "~1.0.0",
        "karma-phantomjs-launcher": "~1.0.0",
        "karma-source-map-support": "~1.1.0",
        "karma-sourcemap-loader": "~0.3.6",
        "karma-spec-reporter": "0.0.26",
        "karma-webpack": "~1.7.0z",
        "loader-utils": "^0.2.15",
        "ng-annotate": "~1.2.1",
        "ng-annotate-webpack-plugin": "~0.1.2",
        "ngtemplate-loader": "~1.3.1",
        "node-sass": "3.4.2",
        "path": "~0.12.7",
        "phantomjs-prebuilt": "2.1.7",
        "sass-loader": "~4.0.0",
        "sinon": "~1.17.3",
        "source-map": "^0.5.6",
        "style-loader": "~0.13.1",
        "url-loader": "~0.5.7",
        "webpack": "~1.13.0",
        "yargs": "~4.7.1"
    },
    "dependencies": {
        "angular": "~1.5.8",
        "angular-animate": "~1.5.8",
        "angular-cookies": "~1.5.8",
        "angular-messages": "~1.5.8",
        "angular-mocks": "~1.5.8",
        "angular-moment": "~1.0.0-beta.6",
        "angular-resource": "~1.5.8",
        "angular-sanitize": "~1.5.8",
        "angular-translate": "~2.11.0",
